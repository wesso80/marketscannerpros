{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(pricing|docs|disclaimer|privacy|terms|contact)", "destination": "/$1" },
    { "source": "/(.*)", "destination": "https://market-scanner-1-wesso80.replit.app/$1" }
  ],
  "redirects": [
    { "source": "/launch", "destination": "https://app.marketscannerpros.app", "permanent": false }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" }
      ]
    }
  ]
}
