{
  "expo": {
    "name": "Market Scanner Pro",
    "slug": "marketscanner-mobile",
    "owner": "wesso80",
    "version": "1.0.1",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a"
    },
    "ios": {
      "bundleIdentifier": "com.wesso80.marketscannermobile",
      "buildNumber": "4",
      "userInterfaceStyle": "dark",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIUserInterfaceStyle": "Dark"
      }
    },
    "android": {
      "package": "com.wesso80.marketscanner",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a1a1a"
      }
    },
    "plugins": [
      "expo-in-app-purchases",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "feb78df7-8426-4d88-8a2e-1fd260bf71df"
      }
    }
  }
}
