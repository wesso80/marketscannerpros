# Android signing configuration
# Copy this file to gradle.properties and update with your keystore details

# Keystore file (relative to android directory)
MARKET_SCANNER_KEYSTORE_PATH=market-scanner-release.keystore

# Keystore and key passwords  
MARKET_SCANNER_KEYSTORE_PASSWORD=your_keystore_password
MARKET_SCANNER_KEY_ALIAS=market-scanner
MARKET_SCANNER_KEY_PASSWORD=your_key_password

# Alternative: Use environment variables instead
# export MARKET_SCANNER_KEYSTORE_PATH="/path/to/keystore"
# export MARKET_SCANNER_KEYSTORE_PASSWORD="password"
# export MARKET_SCANNER_KEY_ALIAS="market-scanner" 
# export MARKET_SCANNER_KEY_PASSWORD="password"